<div id="{{ elementId }}">{% render options.url %}</div>

<script type="text/javascript">
    require(['jquery', 'oro/widget-manager', 'oro/{{ widgetType ~ '-widget' }}'],
    function($, widgetManager, Widget){
        $(function(){
            var widget = new Widget({{ options|json_encode|raw }});
            widgetManager.addWidgetInstance(widget);
            widget.render();
        })
    });
</script>
